<script>
  function getRedirectBrand(hostName) {
    if (hostName.includes('pa')) {
      return 'lbel'
    }

    return 'esika'
  }

  function getRedirectDomain(storeName) {
    if (storeName.includes('compra')) {
      return 'compra'
    }

    return 'belcorp'
  }

  ;(function () {
    const href = window.location.href.toLocaleLowerCase()
    const hostname = window.location.hostname.toLocaleLowerCase()
    const splittedHostname = hostname.split('.')
    const store = splittedHostname.shift()

    if (
      !store ||
      !hostname ||
      !href ||
      href.includes('login') ||
      href.includes('checkout') ||
      href.includes('account') ||
      href.includes('auth')
    ) {
      return
    }

    if (store.includes('compra') || store.includes('belcorp')) {
      const redirectBrand = getRedirectBrand(hostname)
      const redirectDomain = getRedirectDomain(store)
      const redirectUrl = store.replace(redirectDomain, redirectBrand)

      splittedHostname.unshift(redirectUrl)

      window.location.href = `https://${splittedHostname.join('.')}`
    }
  })()
</script>

